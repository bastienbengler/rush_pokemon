#include "Pokemon.hpp"
#include <iostream>

void	Pokemon::setId(const int id)
{
  this->id = id;
}

int	Pokemon::getId() const
{
  return id;
}

void	Pokemon::setName(const std::string& name)
{
  this->name = name;
}

std::string&	Pokemon::getName() const
{
  return name;
}
